<table class="table table-bordered">
    <thead>
    <tr>
        <th>No</th>
        <th>Title</th>
        <th>Contents</th>
    </tr>
    </thead>

    <tbody>
    @foreach (var note in Model)
    {
        <tr>
            <td>@note.NoteNo</td>
            <td>@note.NoteTitle</td>
            <td>@note.NoteContents</td>
        </tr>
    }
    </tbody>
    
</table>

<a class="btn btn-warning" asp-controller="Note" asp-action="Add">Post</a>